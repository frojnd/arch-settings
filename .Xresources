!my eye candy
URxvt.scrollBar_righ: false
URxvt.scrollBar: false
URxvt.transparent: true
URxvt.shading: 25
URxvt*background:   rgba:2222/2222/2222/dddd
!URxvt*background:   rgba:0000/0000/0000/bbb

URxvt*depth:            32

*foreground:              #a0a0a0
!*foreground:              #F94848
!black
*color0:                  #1B1D1E
*color8:                  #505354
!red
*color1:                  #F94848
*color9:                  #CB4C5B
!green
*color2:                  #82B414
*color10:                 #B6E354
!yellow
*color3:                  #FD971F
*color11:                 #FEED6C
!blue
*color4:                  #4E82AA
*color12:                 #0C73C2
!magenta
*color5:                  #8C54FE
*color13:                 #9E6FFE
!cyan
*color6:                  #465457
*color14:                 #899CA1
!white
*color7:                  #CCCCC6
*color15:                 #F8F8F2

URxvt.perl-ext-common: default,matcher,clipboard
URxvt.urlLauncher: chromium
URxvt.matcher.button: 1
URxvt.underlineURLs: true

URxvt*scrollstyle:plainxvt.backgreund: rgba:0000/0000/0000/bbxx

!fonts 
URxvt.font: xft:DejaVu Sans Mono:size=10
!URxvt.font: xft:Terminus:size=12
